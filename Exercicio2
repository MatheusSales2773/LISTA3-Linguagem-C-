#include <stdio.h>

int main() {
    char A = 'A'; 
    char *t = &A; 
    char **s = &t; 
    char ***r = &s; 
    char ****q = &r; 
    char *****p = &q; 

    printf("%c\n", *****p); 
    printf("%c\n", ****q);  
    printf("%c\n", ***r);   
    printf("%c\n", **s);    
    printf("%c\n", *t);     

    return 0;
}
